{
  "name": "obsidian-plugin",
  "version": "0.0.0",
  "main": "build/main.js",
  "description": "Obsidian plugin for personal use.",
  "license": "MIT",
  "scripts": {
    "dev": "bun scripts/esbuild.config.mts",
    "build": "tsc && bun scripts/esbuild.config.mts prod",
    "version": "bun scripts/version-bump.mjs && git add manifest.json versions.json",
    "format": "biome format --write",
    "lint": "biome lint"
  },
  "dependencies": {
    "ulid": "^2.3.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "^1.1.14",
    "esbuild": "^0.24.0",
    "obsidian": "^1.7.2",
    "typescript": "5.7.2"
  }
}
